{
  "name": "field-transform",
  "version": "1.0.0-alpha",
  "description": "A javascript tool to manipulate JSON object by transforming fields.",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "unpkg": "./dist/index.min.js",
  "scripts": {
    "build": "npm run build:es && npm run build:lib && npm run build:dist",
    "build:es": "tsc -p tsconfig.json",
    "build:lib": "rollup --config rollup.config.lib.js",
    "build:dist": "rollup --config rollup.config.dist.js",
    "test": "jest",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [
    "field",
    "transform",
    "field-transform"
  ],
  "author": "calvin_von",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^26.0.24",
    "jest": "^27.0.6",
    "rollup": "^2.53.1",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  }
}
